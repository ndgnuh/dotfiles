theprompt(){
	if [ "$EUID" -ne 0 ]; then
 		echo '%K{green}%F{black} %1~$(git_prompt_info) %f%k %F{green}%#%f '
	else	
		echo '%K{red} %1~$(git_prompt_info) %f%k %F{red}%#%f '
	fi
}
